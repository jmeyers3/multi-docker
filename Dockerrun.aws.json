{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
        {
            "name":"client",
            "image": "jayship2000/multi-client",
            "hostname":"client",
            "essential": false
        },
        {
            "name":"server",
            "image":"jayship2000/multi-server",
            "hostname":"api",
             "essential": false
        },
        {
            "name":"worker",
            "image":"jayship2000/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image":"jayship2000/multi-nginx",
            "hostname": "nginx", 
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client",
                "server"
            ]

        }
    ]
}